<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Labo" table="labo">
    <id name="laboId" type="integer" column="labo_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etabExt" type="text" column="etab_ext" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lien" type="string" column="lien" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mailcontact" type="string" column="mailContact" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="responsable" type="string" column="responsable" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="integer" column="code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="effectif" type="integer" column="effectif" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sigle" type="string" column="sigle" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lien2" type="string" column="lien2" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lien3" type="string" column="lien3" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="uai" type="string" column="uai" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-many field="membre" target-entity="Membre" mapped-by="labo" fetch="LAZY"/>
    <many-to-many field="tag" target-entity="Tag" mapped-by="labo" fetch="LAZY"/>
    <many-to-many field="ufr" target-entity="Ufr" mapped-by="labo" fetch="LAZY"/>
    <many-to-many field="localisation" target-entity="Localisation" mapped-by="labo" fetch="LAZY"/>
    <many-to-many field="formation" target-entity="Formation" inversed-by="labo" fetch="LAZY">
      <join-table name="labo_has_formation">
        <join-columns>
          <join-column name="labo_id" referenced-column-name="labo_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="formation_id" referenced-column-name="formation_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="discipline" target-entity="Discipline" mapped-by="labo" fetch="LAZY"/>
    <many-to-many field="etablissement" target-entity="Etablissement" mapped-by="labo" fetch="LAZY"/>
    <many-to-many field="equipement" target-entity="Equipement" inversed-by="labo" fetch="LAZY">
      <join-table name="labo_has_equipement">
        <join-columns>
          <join-column name="labo_id" referenced-column-name="labo_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="equipement_id" referenced-column-name="equipement_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="ed" target-entity="Ed" inversed-by="labo" fetch="LAZY">
      <join-table name="ed_has_labo">
        <join-columns>
          <join-column name="labo_id" referenced-column-name="labo_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="ED_id" referenced-column-name="ED_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
