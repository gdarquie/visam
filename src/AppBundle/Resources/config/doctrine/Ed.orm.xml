<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ed" table="ED">
    <id name="edId" type="integer" column="ED_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="string" column="code" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Code l’école doctorale</option>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lien" type="string" column="lien" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="laboExt" type="text" column="labo_ext" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Autres labo hors HESAM qui sont en lien avec l’ED
</option>
      </options>
    </field>
    <field name="etabExt" type="text" column="etab_ext" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Etablissements partenaires

Paris 8 (234D); Paris 9 (244D)</option>
      </options>
    </field>
    <field name="contact" type="string" column="contact" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="effectif" type="integer" column="effectif" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">Nombre de doctorants
</option>
      </options>
    </field>
    <many-to-many field="membre" target-entity="Membre" mapped-by="ed" fetch="LAZY"/>
    <many-to-many field="localisation" target-entity="Localisation" mapped-by="ed" fetch="LAZY"/>
    <many-to-many field="etablissement" target-entity="Etablissement" mapped-by="ed" fetch="LAZY"/>
    <many-to-many field="discipline" target-entity="Discipline" mapped-by="ed" fetch="LAZY"/>
    <many-to-many field="labo" target-entity="Labo" mapped-by="ed" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
