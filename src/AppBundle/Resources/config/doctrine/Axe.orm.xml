<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Axe" table="axe">
    <indexes>
      <index name="fk_axe_labo1_idx" columns="labo_id"/>
    </indexes>
    <id name="axeId" type="integer" column="axe_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="text" column="nom" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="labo" target-entity="Labo" fetch="LAZY">
      <join-columns>
        <join-column name="labo_id" referenced-column-name="labo_id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="tag" target-entity="Tag" inversed-by="axe" fetch="LAZY">
      <join-table name="axe_has_tag">
        <join-columns>
          <join-column name="axe_id" referenced-column-name="axe_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="tag_id" referenced-column-name="tag_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
