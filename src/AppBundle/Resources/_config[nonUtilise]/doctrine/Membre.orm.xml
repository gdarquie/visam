<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Membre" table="membre">
    <id name="membreId" type="integer" column="membre_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="profession" type="string" column="profession" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-many field="labo" target-entity="Labo" inversed-by="membre" fetch="LAZY">
      <join-table name="membre_has_labo">
        <join-columns>
          <join-column name="membre_id" referenced-column-name="membre_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="labo_id" referenced-column-name="labo_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="formation" target-entity="Formation" inversed-by="membre" fetch="LAZY">
      <join-table name="membre_has_formation">
        <join-columns>
          <join-column name="membre_id" referenced-column-name="membre_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="formation_id" referenced-column-name="formation_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="ed" target-entity="Ed" inversed-by="membre" fetch="LAZY">
      <join-table name="membre_has_ed">
        <join-columns>
          <join-column name="membre_id" referenced-column-name="membre_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="ED_id" referenced-column-name="ED_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
