<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ufr" table="ufr">
    <indexes>
      <index name="fk_ufr_etablissement1_idx" columns="etablissement_id"/>
    </indexes>
    <id name="ufrId" type="integer" column="ufr_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="string" column="code" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lien" type="string" column="lien" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contact" type="string" column="contact" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="effectif" type="integer" column="effectif" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etudiants" type="integer" column="etudiants" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="etablissement" target-entity="Etablissement" fetch="LAZY">
      <join-columns>
        <join-column name="etablissement_id" referenced-column-name="etablissement_id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="labo" target-entity="Labo" inversed-by="ufr" fetch="LAZY">
      <join-table name="ufr_has_labo">
        <join-columns>
          <join-column name="ufr_id" referenced-column-name="ufr_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="labo_id" referenced-column-name="labo_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="discipline" target-entity="Discipline" inversed-by="ufr" fetch="LAZY">
      <join-table name="ufr_has_discipline">
        <join-columns>
          <join-column name="ufr_id" referenced-column-name="ufr_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="discipline_id" referenced-column-name="discipline_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="localisation" target-entity="Localisation" mapped-by="ufr" fetch="LAZY"/>
    <many-to-many field="formation" target-entity="Formation" mapped-by="ufr" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
