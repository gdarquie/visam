<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Equipement" table="equipement">
    <id name="equipementId" type="integer" column="equipement_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Au cas o√π</option>
      </options>
    </field>
    <field name="description" type="text" column="description" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lien" type="string" column="lien" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="labo" target-entity="Labo" mapped-by="equipement" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
