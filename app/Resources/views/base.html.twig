        {% for type, messages in app.session.flashBag.all %}
            {% for message in messages %}
                <div class="{{ type }}">
                    {{ message|trans({}, 'FOSUserBundle') }}
                </div>
            {% endfor %}
        {% endfor %}

        <div>

        {% block fos_user_content %}{% endblock fos_user_content %}

            <ul id="menu_add" class="dropdown-content">
                <li><a href="{{ path('editeur_etablisement_new') }}">Etablissement</a></li>
                <li><a href="">Formation</a></li>
                <li><a href="">Laboratoire</a></li>
            </ul>

            <div class="navbar-fixed">
            <nav>
                <div class="nav-wrapper red-hesam">
                    <a href="{{ path('homepage') }}" class="brand-logo">hesam</a>
                    <ul id="nav-mobile" class="right hide-on-med-and-down">
                        <li><a href="{{ path('search') }}">Rechercher</a></li>
                        <li><a href="{{ path("statistiques") }}">Statistiques</a></li>
                        <li><a href="{{ path("apropos") }}">A propos</a></li>
                        <li><a href="#">Mode d'emploi</a></li>
                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                            <li><a class="dropdown-button" href="#!" data-activates="menu_add">Ajouter<i class="material-icons right">arrow_drop_down</i></a></li>
                            <li>connecté : {{ app.user.username }}</li>
                            <li><a href="{{ path('fos_user_security_logout') }}">logout</a></li>
                        {% else %}
                            <li><a href="{{ path('fos_user_security_login') }}">connexion</a></li>
                        {% endif %}
                    </ul>
                </div>
            </nav>
            </div>

        {% block body %}{% endblock %}

        <footer class="page-footer red-hesam">

            <div class="container row">

                <div class="col s7">

                    <h2>Les établissements membres</h2>
                        <li>Université Paris 1 Panthéon - Sorbone</li>
                        <li>Conservatoire National des Arts et Métiers</li>
                        <li>Ecole Supérieure Commerce de Paris (ESCP Europe)</li>
                        <li>Ecole du Louvre</li>
                        <li>Ecole Nationale d'Administration</li>
                        <li>Ecole Nationale Supérieure de Création Industrielle (ENSCI les Ateliers)</li>
                        <li>Ecole Nationale Supérieure d'Architecture Paris la Villette (ENSAPLV)</li>
                        <li>Institut National d'Etudes Démographiques (INED)</li>
                        <li>Institut National du Patrimoine (INP)</li>
                        <li>Arts et Métiers ParisTech(ENSAM)</li>
                        <li>Institut National d'Histoire de l'Art (INHA)</li>
                        <li>Centre National Recherche Scientifique</li>
                </div>

                <div class="col s5">

                    <div class="row accueil-liens center">
                        <p><a class="grey-text text-lighten-4" href="{{ path('editeur') }}">Accès réservé</a></p>
                    </div>

                    <div class="row">
                        <h2>Contacts</h2>

                        <span class="contactHesam center">
                        <p>heSam Université</p>
                        <p>15 rue Soufflot</p>
                        <p>75005 Paris</p>
                        <p>01 56 81 20 00</p>
                        <p>contact@hesam.eu</p>
                        </span>
                    </div>


                    <div>
                        <p>Le projet a bénéficié du soutien de l’ANR au titre du programme Investissements d’avenir (ANR-11-IDEX-0006).</p>
                    </div>


                    <div class="row">
                        <span>
                            <p>© 2016 heSam Université - Vous êtes sur une version test du site, plusieurs développements sont toujours en cours</p>
                        </span>
                    </div>

                </div>

            </div>
            <div class="footer-copyright">
                <div class="container"></div>
                <div class="container">
                    © 2016 heSam Université - Vous êtes sur une version test du site, plusieurs développements sont toujours en cours
                    <a class="grey-text text-lighten-4 right" href="http://www.hesam.eu" target="_blank">Accéder au site hesam</a>
                </div>
            </div>

        </footer>

        {% block javascripts %}
            {#<script type="text/javascript" src="{{ asset("assets/vendor/jquery/dist/jquery.min.js") }}"></script>#}{# problème avec stats si on utilise ce jquery#}
            <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
            <script type="text/javascript" src="{{ asset("assets/vendor/materialize/dist/js/materialize.min.js") }}"></script>
            <script type="text/javascript" src="{{ asset("js/function.js") }}"></script>
        {% endblock %}

        </div>
    </body>
