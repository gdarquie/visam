<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Discipline" table="discipline">
    <indexes>
      <index name="fk_discipline_domaine1_idx" columns="domaine_id"/>
    </indexes>
    <id name="disciplineId" type="integer" column="discipline_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="integer" column="code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Type de discipline - CNU, SISE, HCERS domaine (grosses disciplines de SISE 17).
</option>
      </options>
    </field>
    <field name="abreviation" type="string" column="abreviation" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="domaineId" type="integer" column="domaine_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hesametteId" type="integer" column="hesamette_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="ufr" target-entity="Ufr" mapped-by="discipline" fetch="LAZY"/>
    <many-to-many field="labo" target-entity="Labo" inversed-by="discipline" fetch="LAZY">
      <join-table name="discipline_has_labo">
        <join-columns>
          <join-column name="discipline_id" referenced-column-name="discipline_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="labo_id" referenced-column-name="labo_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="formation" target-entity="Formation" inversed-by="discipline" fetch="LAZY">
      <join-table name="discipline_has_formation">
        <join-columns>
          <join-column name="discipline_id" referenced-column-name="discipline_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="formation_id" referenced-column-name="formation_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="ed" target-entity="Ed" inversed-by="discipline" fetch="LAZY">
      <join-table name="discipline_has_ed">
        <join-columns>
          <join-column name="discipline_id" referenced-column-name="discipline_id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="ED_id" referenced-column-name="ED_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
